}

.class-bar-wrap{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color: #fff;
  margin-bottom:15rpx;
  width: 92%;
  margin-left: auto;
  margin-right: auto;
  border-radius: 25rpx;
}

.button-wrap{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 30%;
  height: auto;
  background-color: #fff;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
}

.reserve-button{
  height: 50%;
  width: auto;
  text-align: center;
  line-height: 3;
  color: #499ef3;
  font-size:35rpx;
  border-left: #499ef3 solid 2rpx;
  border-bottom: #499ef3 solid 2rpx;
  font-weight: bold;
}

.detail-button{
  height: 50%;
  width: auto;
  text-align: center;
  line-height: 3;
  color: #499ef3;
  font-size:35rpx;
  border-left:#499ef3 solid 2rpx;
  font-weight: bold;
}

