.navbar {
  text-align: center;
  color: #6d6d72;
  display: flex;
  justify-content: space-around;
  padding-bottom: 40rpx;
  position: fixed;
  background-color: #efeff4;
  padding-top: 120rpx;
  width: 100%;
  z-index: 99;
}

.navbar .item {
  position: relative;
  text-align: center;
  line-height: 30rpx;
  font-size: 40rpx;
  font-weight: lighter;
}

.navbar .item.active {
  font-weight: bolder;
}

.navbar .item.active::after {
  content: "";
  display: block;
  position: absolute;
  bottom: -20rpx;
  left: 0;
  right: 0;
  height: 5rpx;
  background: #6d6d72;
}

.line.search-bar-wrap {
  position: fixed;
