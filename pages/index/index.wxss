@import "/template/tabbar.wxss";
.root {
  left: 0;
  right: 0;
  padding-top: 20rpx; 
  background: #efeff4;
  height: 100%; 
}

.root-ip4 {
   padding-top: 30rpx; 
}

.root-ipx {
   padding-top: 60rpx; 
}

.sysbar-hider{
  height: 40rpx; 
  position: fixed; 
  width: 100%; 
  top: 0; 
  background-color: #efeff4; 
  z-index: 9999
}

.sysbar-hider-ipx {
  height: 100rpx;
}

.sysbar-hider-ip4 {
  height: 50rpx;
}

.tabbar-holder{
  height: 200rpx;
}

.title {
  color: #6d6d72;
  font-size: 60rpx;
  padding-left: 40rpx;
  padding-bottom: 15rpx;
  padding-top: 15rpx;
  font-weight: bold;
}

.line {
  padding-top: 20rpx;
}

.line text {
  color: #6d6d72;
  font-size: 28rpx;
  margin-left: 40rpx;
}

.line .demo {
  height: 110rpx;
  line-height: 110rpx;
  box-sizing: border-box;
  text-align: left;
  color: #333;
  background-color: #fff;
  position: relative;
  font-size: 36rpx;
  padding-left: 40rpx;
  margin-top: 10rpx;
  font-weight: bold;
  width:92%;
  margin-left:auto;
  margin-right:auto;
  border-radius:17rpx;
}

.line input.demo{
  font-weight: lighter;
}

.line .item-header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color: #fff;
  border-top-left-radius: 25rpx;
}

.line .item-header .cancel {
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
  text-align: left;
  color: #333;
  background-color: #fff;
  position: relative;
  font-size: 32rpx;
  padding-right: 40rpx;
  margin-bottom: 20rpx;
}

.line .item-header .demo-1 {
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
  text-align: left;
  color: #333;
  background-color: #fff;
  position: relative;
  font-size: 45rpx;
  padding-left: 35rpx;
  font-weight: bold;
  border-top-left-radius: 25rpx;
}

.line .info-wrap {
  border-top-left-radius: 25rpx;
  border-bottom-left-radius: 25rpx;
  max-width: 500rpx;
}

.line .title-wrap {
  display: flex;
  flex-direction: row;
  position: fixed;
  background-color: #efeff4;
  width: 100%;
  z-index: 100;
}

.line .title-wrap .title-back {
  padding-left: 0;
}

.line .title-wrap .upload-button {
  font-size: 32px !important;
  line-height: 90rpx;
  padding-right: 23rpx;
  padding-top: 15rpx;
}

.line .title-wrap .back-button {
  font-size: 45rpx !important;
  line-height: 85rpx;
  padding-right: 20rpx;
  padding-top: 15rpx;
  padding-left: 20rpx;
  color: #6d6d72;
}

.line .title-wrap .refresh-button {
  font-size: 45rpx !important;
  line-height: 90rpx;
  padding-left: 10rpx;
  padding-top: 15rpx;
  color: #6d6d72;
  animation: spin 800ms infinite linear;  
  animation-play-state: paused;
  -webkit-animation-play-state: paused;
}

.line .title-wrap .refresh-button.active {
  animation-play-state: running;
  -webkit-animation-play-state: running;
}

@keyframes spin {
  0% {
    transform: rotate(360deg);
    transform-origin: 60% 55%;
    -webkit-transform: rotate(360deg);
    -webkit-transform-origin: 60% 55%;  
  }
  100% {
    transform: rotate(0deg);
    transform-origin: 60% 55%;  
    -webkit-transform: rotate(0deg);
    -webkit-transform-origin: 60% 55%;  
  }
}

.line .demo-2 {
  box-sizing: border-box;
  text-align: left;
  color: #333;
  background-color: #fff;
  position: relative;
  font-size: 32rpx;
  padding-left: 35rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 50rpx;
  margin-bottom: 0;
  border-bottom-left-radius: 25rpx;
}

.line .demo-2 .class-status {
  padding-right: 35rpx;
}

.line .demo .link {
  position: absolute;
  right: 40rpx;
  top: 0rpx;
  color: #8e8e93;
  font-weight: lighter;
}

.hide {
  display: none;
}

@media (max-width : 360px) {
  .line text {
    font-size: 36rpx;
  }

  .line .demo {
    height: 110rpx;
    line-height: 110rpx;
    font-size: 38rpx;
    padding-left: 40rpx;
    font-weight: bold;
  }
}

.empty-note {
  color: #6d6d72;
  font-size: 40rpx;
  padding-top: 80rpx;
  text-align: center;
}

.empty-intro {
  color: #6d6d72;
  font-size: 40rpx;
  padding-bottom: 15rpx;
  text-align: center;
}

.empty-icon {
  margin: 0 auto;
  width: 120rpx;
  height: 120rpx;
